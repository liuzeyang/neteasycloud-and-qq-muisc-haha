.navbar {
  display: flex;
  background: #fff;
  height: 80rpx; }
  .navbar .item {
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 80rpx; }
    .navbar .item.active {
      color: #000; }
      .navbar .item.active:after {
        content: "";
        display: block;
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        height: 4rpx;
        background: #000; }

.push .swiper {
  height: 300rpx; }
  .push .swiper .img {
    width: 100%;
    height: 100%; }

.push .radio {
  margin: 30rpx 20rpx; }
  .push .radio .title {
    display: flex;
    margin-bottom: 20rpx; }
  .push .radio .list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between; }
    .push .radio .list .item {
      background: #fff; }
      .push .radio .list .item img {
        position: relative;
        vertical-align: top;
        width: 350rpx;
        height: 350rpx; }
      .push .radio .list .item .list-media {
        position: relative;
        width: 350rpx;
        height: 350rpx;
        box-sizing: border-box;
        overflow: hidden; }
        .push .radio .list .item .list-media .list-count {
          position: absolute;
          left: 10rpx;
          bottom: 10rpx;
          font-size: 24rpx;
          color: #fff;
          z-index: 10; }
      .push .radio .list .item .text {
        display: block;
        padding: 10rpx 14rpx;
        font-size: 28rpx;
        width: 322rpx;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap; }
    .push .radio .list .songitem {
      margin-bottom: 20rpx; }
    .push .radio .list .author {
      display: block;
      padding: 0 14rpx 20rpx;
      font-size: 24rpx; }

.push .radio .list .item .list-media .list-count:before {
  display: inline-block;
  margin-right: 6rpx;
  content: '';
  width: 20rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain; }

.push .radio .list .item .list-media:after {
  content: '';
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
  width: 48rpx;
  height: 48rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain; }

.wx-swiper-dot::before {
  color: #fff; }

.wx-swiper-dot-active::before {
  color: #000; }

.topList {
  margin: 20rpx; }
  .topList .item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 2rpx;
    background: #fff; }
  .topList .media {
    flex: 0 0 200rpx;
    position: relative;
    width: 200rpx;
    height: 100%; }
    .topList .media .img {
      flex: 0 0 200rpx;
      position: relative;
      width: 200rpx;
      height: 200rpx; }
    .topList .media .count {
      position: absolute;
      bottom: 10rpx;
      left: 10rpx;
      font-size: 24rpx;
      line-height: 1;
      color: #fff;
      opacity: 0.6; }
  .topList .info {
    flex: auto;
    margin-left: 30rpx;
    overflow: hidden; }
    .topList .info .title {
      display: block;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
      color: #000;
      margin-bottom: 10rpx; }
    .topList .info .text {
      display: block;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
      font-size: 28rpx;
      color: rgba(0, 0, 0, 0.5); }
      .topList .info .text text {
        color: #000;
        margin-left: 16rpx;
        margin-right: 10rpx; }
  .topList .arrow {
    flex: 0 0 12rpx;
    margin-right: 20rpx;
    width: 12rpx;
    height: 12rpx;
    border-right: 1px solid #b2b2b2;
    border-bottom: 1px solid #b2b2b2;
    transform: rotate(-45deg); }

.topList .media .count:before {
  display: inline-block;
  margin-right: 6rpx;
  content: '';
  width: 20rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain; }


.search {
    display: flex;
    flex-direction: column;
    height: 93.5%;
    background: #fff;
}

.search .search-bar {
    position: relative;
    padding: 16rpx 20rpx;
    display: flex;
    box-sizing: border-box;
    background-color: #efeff4;
}

.search .search-wrap {
    position: relative;
    flex: auto;
    border-radius: 10rpx;
    background: #fff;
    border: 1rpx solid #e6e6ea;
}

.search .search-box {
    position: relative;
    display: flex;
    box-sizing: border-box;
}

.search .icon-search {
    margin: 16rpx 16rpx 16rpx 20rpx;
}

.search .search-input {
    flex: 1;
    padding-top: 6rpx;
    padding-bottom: 6rpx;
    padding-right:  60rpx;
    font-size: 28rpx;
    height: 56rpx;
    line-height: 56rpx;
}

input::-webkit-input-placeholder {
    color: rgb(117, 117, 117);
}

.search .icon-clear {
    position: absolute;
    top: 12rpx;
    right: 16rpx;
}

.search .cancel-btn {
    margin-left: 20rpx;
    line-height: 70rpx;
    color: #09bb07;
    white-space: nowrap;
    font-size: 28rpx;
    box-sizing: border-box;
}

